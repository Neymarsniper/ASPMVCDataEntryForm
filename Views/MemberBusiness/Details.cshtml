@model MemberDataEntryForm.Models.MemberBusiessData

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>MemberBusiessData</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Member No.
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MemNo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class = "col-sm-2">
            Business Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BusinessName)
        </dd>
        <dt class = "col-sm-2">
            Business Detail
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BusinessDetail)
        </dd>
        <dt class = "col-sm-2">
            Business Address
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BusinessAddress)
        </dd>
        <dt class = "col-sm-2">
            Business City
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BusinessCity)
        </dd>
        <dt class = "col-sm-2">
            Business Postal Code
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BusinessPostalCode)
        </dd>
        <dt class = "col-sm-2">
            Business E-mail
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BusinessEmail)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-secondary">Edit Details</a> |
    <a asp-controller="Home" asp-action="Index" class="btn btn-primary">Back to Member Details</a>
</div>
